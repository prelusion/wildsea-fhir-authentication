    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.16"
  },
  "repository": {
